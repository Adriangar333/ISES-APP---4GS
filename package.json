{
  "name": "route-assignment-system",
  "version": "1.0.0",
  "description": "Sistema asignador de rutas por zonas para interventores",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --noEmitOnError false",
    "start": "node dist/server.js",
    "vercel-build": "tsc",
    "start:simple": "node dist/simple-server.js",
    "build:railway": "tsc --skipLibCheck --noEmitOnError false",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "dev:full": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "setup:db": "node scripts/setup-db.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "routes",
    "assignment",
    "zones",
    "inspectors"
  ],
  "author": "ISES",
  "license": "MIT",
  "dependencies": {
    "@tmcw/togeojson": "^7.1.2",
    "@types/socket.io": "^3.0.1",
    "@types/xlsx": "^0.0.35",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.6.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-apn": "^3.0.0",
    "nodemailer": "^7.0.6",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "xlsx": "^0.18.5",
    "xmldom": "^0.6.0",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
